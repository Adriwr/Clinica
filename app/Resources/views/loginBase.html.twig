        <link href="{{ asset('font-awesome/css/font-awesome.css') }}" rel="stylesheet">
        <link href="{{ asset('css/vendor/plugins/metisMenu/dist/metisMenu.min.css') }}" rel="stylesheet">

        {# CSS principal #}
        <link href="{{ asset('css/app/style-2.css') }}" rel="stylesheet">

